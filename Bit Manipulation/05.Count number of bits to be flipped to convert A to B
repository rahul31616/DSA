#include <bits/stdc++.h>
using namespace std;

class Solution {
public:
    // Count flips needed to convert A to B
    int bitDifference(int A, int B) {
        int xorVal = A ^ B;
        // Count set bits in xorVal
        return __builtin_popcount(xorVal);
    }
};

int main() {
    int A, B;
    cin >> A >> B;
    Solution sol;
    cout << sol.bitDifference(A, B) << "\n";
    return 0;
}
