#include <bits/stdc++.h>
using namespace std;

class Solution {
public:
    // Brian Kernighan’s algorithm — usually very efficient
    int countSetBits(int n) {
        int count = 0;
        while (n) {
            n &= (n - 1);
            count++;
        }
        return count;
    }
};

int main() {
    int n;
    cin >> n;
    Solution sol;
    cout << sol.countSetBits(n) << "\n";
    return 0;
}
