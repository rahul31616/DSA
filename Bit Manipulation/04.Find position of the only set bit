#include <bits/stdc++.h>
using namespace std;

class Solution {
public:
    int findPosition(int n) {
        // Ensure exactly one set bit
        if (n == 0 || (n & (n - 1)) != 0)
            return -1;

        int pos = 1;
        while ((n & 1) == 0) {
            n >>= 1;
            pos++;
        }
        return pos;
    }
};

int main() {
    int n;
    cin >> n;
    Solution sol;
    cout << sol.findPosition(n) << "\n";
    return 0;
}
