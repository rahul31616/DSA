#include <bits/stdc++.h>
using namespace std;

string printSequence(const string& input) {
    // Mapping for each letter A-Z (index 0 = A, 25 = Z)
    string keypad[] = {
        "2", "22", "222",     // A, B, C
        "3", "33", "333",     // D, E, F
        "4", "44", "444",     // G, H, I
        "5", "55", "555",     // J, K, L
        "6", "66", "666",     // M, N, O
        "7", "77", "777", "7777", // P, Q, R, S
        "8", "88", "888",     // T, U, V
        "9", "99", "999", "9999"  // W, X, Y, Z
    };

    string output = "";

    for (char c : input) {
        if (c == ' ') {
            output += "0";
        } else {
            c = toupper(c);  // Convert to uppercase
            output += keypad[c - 'A'];
        }
    }

    return output;
}

int main() {
    string sentence = "HELLO WORLD";
    cout << printSequence(sentence) << endl;
    return 0;
}
