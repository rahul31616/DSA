#include <bits/stdc++.h>
using namespace std;

int minOpsTransform(string A, string B) {
    int n = A.size();
    if (B.size() != n) return -1;

    // Check if transformation possible (same char freq)
    vector<int> count(256, 0);
    for (int i = 0; i < n; i++) {
        count[A[i]]++;
        count[B[i]]--;
    }
    for (int x : count)
        if (x != 0) return -1; // not possible

    int i = n - 1, j = n - 1;
    int operations = 0;

    while (i >= 0) {
        if (A[i] == B[j]) {
            i--; j--; // match, move both
        } else {
            i--;      // skip from A
            operations++;
        }
    }
    return operations;
}

int main() {
    string A = "EACBD", B = "EABCD";
    cout << minOpsTransform(A, B) << endl;  // 3

    A = "ABD"; B = "BAD";
    cout << minOpsTransform(A, B) << endl;  // 1
}
